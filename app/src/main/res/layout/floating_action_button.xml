    xmlns:app="http://schemas.android.com/tools"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:id="@+id/multiple_actions"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="end|bottom"
    android:layout_margin="16dp"
    app:layout_behavior="com.awesome_folks.quidity.FloatingActionButtonBehavior"
    fab:fab_addButtonColorNormal="@color/accentColor"
    fab:fab_addButtonColorPressed="@color/accent_pressed"
    fab:fab_addButtonPlusIconColor="@color/white"
    fab:fab_labelStyle="@style/menu_labels_style">

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/addNotice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        fab:fab_colorNormal="@color/white"
        fab:fab_colorPressed="@color/white_pressed"
        fab:fab_icon="@drawable/ic_today_white_24dp"
        fab:fab_title="Add Notices" />

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/addNote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        fab:fab_colorNormal="@color/white"
        fab:fab_colorPressed="@color/white_pressed"

        fab:fab_icon="@drawable/ic_note_add_white_24dp"
        fab:fab_title="Add Note" />

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/addDiscussion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        fab:fab_colorDisabled="@color/half_black"
        fab:fab_colorNormal="@color/white"

        fab:fab_colorPressed="@color/white_pressed"
        fab:fab_icon="@drawable/ic_group_add_white_24dp"
        fab:fab_title="Add Discussion" />
